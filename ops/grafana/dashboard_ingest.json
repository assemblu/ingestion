{
  "title": "Ingest Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "graph",
      "title": "Rows inserted (mart.*)",
      "targets": [
        {
          "expr": "rate(clickhouse_profile_events_InsertRowBytes[1m])"
        }
      ],
      "id": 1
    },
    {
      "type": "graph",
      "title": "Kafka lag (trades)",
      "targets": [
        {
          "expr": "kafka_consumergroup_lag{topic=\"trades\"}"
        }
      ],
      "id": 2
    },
    {
      "type": "graph",
      "title": "Replication queue",
      "targets": [
        {
          "expr": "clickhouse_replication_queue_size"
        }
      ],
      "id": 3
    }
  ]
}

